{
  "name": "@cpangular/web-cdk",
  "version": "0.0.22",
  "dependencies": {
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@cpangular/web-utils": "^0.0.22"
  },
  "peerDependencies": {
    "@angular/common": "^12.1.1",
    "@angular/core": "^12.1.1",
    "@cpangular/web-utils": "0.0.22"
  },
  "publishConfig": {
    "access": "public",
    "directory": "../../dist/web-cdk"
  },
  "repository": "https://github.com/cpangular/angular-framework",
  "scripts": {
    "build": "ng build web-cdk",
    "release:build": "ng build web-cdk --configuration=production",
    "start": "ng build web-cdk --watch",
    "dev:link": "cd ../../dist/web-cdk && yarn link",
    "dev:unlink": "cd ../../dist/web-cdk && yarn unlink"
  }
}
